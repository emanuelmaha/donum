  "angular-cli": {},
  "scripts": {
    "build-electron": "ng build --base-href . && cp src/electron/* dist",
    "pouchdb-server": "pouchdb-server -p 10101 -d /tmp/pouchdb-server/ -n true",
    "build-win": "electron-packager dist --platform win32 ",
    "package": "npm run build-electron && npm run build-win",
    "start": "electron dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "run-browser": "concurrently \"npm run pouchdb-server\" \"ng serve\" ",
    "run-electron": "concurrently \"npm run pouchdb-server\" \"npm run start\""
  },
